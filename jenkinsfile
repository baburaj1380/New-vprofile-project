pipeline {
    agent any
    tools {
        maven 'maven3'
    }
    stages {
        stage("Build Artifact") {
        steps {
            script {
                sh 'mvn clean package -DskipTests'
            }
        }
        } 
          stage("Test") {
          steps {
            script {
                sh 'mvn test'
            }
        }
        }
   stage("deploy") {
        steps {
          script {
             scp -o StrictHostKeyChecking=no target/java-maven-app-1.1.0-SNAPSHOT.jar ubuntu@52.90.92.201:/home/ubuntu/
            }
            }
        }
   } 
}
